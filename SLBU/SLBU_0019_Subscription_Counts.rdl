    <DataSet Name="LapsedCustomers">
      <Query>
        <DataSourceName>ds_dpt_warehouse_live</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Title">
            <Value>=Parameters!Title.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Medium">
            <Value>=Parameters!Medium.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @MonthEnd date
SET @MonthEnd = GETDATE() - DATEPART(d, GETDATE())
DECLARE @LYMonthEnd date
SET @LYMonthEnd = DATEADD(yy, -1, @MonthEnd)
SELECT DISTINCT o.[Full Name],@MonthEnd AS '12m. Ending'
FROM dim.Organisation o
WHERE 
EXISTS
	(
	SELECT DISTINCT o1.[Full Name], MAX(s1.[Expire Date])
	FROM dim.Organisation o1
	INNER JOIN dim.CustomerAddress ca1 ON o1.[Organisation Key] = ca1.[Organisation Key]
	INNER JOIN fact.Sales s1 ON ca1.[Customer Key] = s1.[Ship To Customer Key]
	INNER JOIN dim.Product p1 ON s1.[Unpackaged Product Key] = p1.[Product Key]
	INNER JOIN dim.ProductGroup pg1 ON p1.[DW Product Group Key] = pg1.[Product Group Key]
	INNER JOIN dim.Medium m1 ON p1.[Medium Key] = m1.[Medium Key]
	WHERE 
		pg1.[Product Title] = @Title
		AND m1.[Medium3 ID] IN (@Medium)
		AND	s1.[Expire Date] &lt;= @LYMonthEnd AND s1.[Expire Date] &gt; DATEADD(yy, -1, @LYMonthEnd)
		AND o1.[Full Name] = o.[Full Name] 
	GROUP BY o1.[Full Name]
	)
AND 
NOT EXISTS
	(
	SELECT DISTINCT o2.[Full Name]
	FROM dim.Organisation o2
	INNER JOIN dim.CustomerAddress ca2 ON o2.[Organisation Key] = ca2.[Organisation Key]
	INNER JOIN fact.Sales s2 ON ca2.[Customer Key] = s2.[Ship To Customer Key]
	INNER JOIN dim.Product p2 ON s2.[Unpackaged Product Key] = p2.[Product Key]
	INNER JOIN dim.ProductGroup pg2 ON p2.[DW Product Group Key] = pg2.[Product Group Key]
	INNER JOIN dim.Medium m2 ON p2.[Medium Key] = m2.[Medium Key]
	WHERE 
		pg2.[Product Title] = @Title AND
		m2.[Medium3 ID] IN (@Medium) AND
		s2.[Start Date] &gt; @LYMonthEnd AND s2.[Start Date] &lt;= @MonthEnd AND
		o2.[Full Name] = o.[Full Name]
	)
ORDER BY o.[Full Name]</CommandText>
      </Query>
      <Fields>
        <Field Name="Full_Name">
          <DataField>Full Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID12m__Ending">
          <DataField>12m. Ending</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
