          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="No_of_Subs">
          <DataField>No of Subs</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NPG_Commission">
          <DataField>NPG Commission</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Invoiced">
          <DataField>Invoiced</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
