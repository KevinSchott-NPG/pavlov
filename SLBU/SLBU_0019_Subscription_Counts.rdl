    <DataSet Name="SubsToDate">
      <Query>
        <DataSourceName>ds_dpt_warehouse_live</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Title">
            <Value>=Parameters!Title.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@Medium">
            <Value>=Parameters!Medium.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--Calculate last month end date for current year and last year
DECLARE @MonthEndThisYear date
DECLARE @MonthEndLastYear date
DECLARE @PeriodStart date
SET @MonthEndThisYear = GETDATE() - DATEPART(d, GETDATE())
SET @MonthEndLastYear = DATEADD(yy, -1, @MonthEndThisYear)
--Calculate previous month's end date
SET @PeriodStart = @MonthEndThisYear
--Calculate first month of previous year
SET @PeriodStart = DATEADD(m, -12 - MONTH(@PeriodStart), GETDATE())
--Calculate 1st January in that year
SET @PeriodStart = DATEADD(d, -DAY(@PeriodStart)+1, @PeriodStart)
;WITH LiveSubs AS
(
SELECT 
	YEAR(s.[Start Date]) AS 'Start Year', od.[NPG Commission], o.[organisation ID]
	, s.[Start Date], s.[Expire Date]
	, CASE L.[npg sl sales1] 
		WHEN 'APAC' THEN 'AP'
		ELSE L.[NPG SL sales1] 
	END AS 'Region'
FROM
	fact.Sales s
	INNER JOIN dim.CustomerAddress ca ON s.[Ship To Customer Key] = ca.[Customer Key]
	INNER JOIN dim.Organisation o ON ca.[Organisation Key] = o.[Organisation Key]
	INNER JOIN dim.OrderDetails od ON s.[Order Details Key] = od.[Order Details Key]
	INNER JOIN dim.Location L ON o.[Location Key] = L.[Location Key]
	INNER JOIN dim.Product p ON S.[Unpackaged Product Key] = p.[Product Key]
	INNER JOIN dim.ProductGroup pg ON p.[DW Product Group Key] = pg.[Product Group Key]
	INNER JOIN dim.Medium m ON p.[Medium Key] = m.[Medium Key]
WHERE
	s.[Start Date] &gt;= @PeriodStart AND
	s.[Cancellation Date] is null AND
	o.[Organisation ID] &gt; 0 AND
	m.[Medium3 ID] IN (@Medium)
	and pg.[Product Title] = @Title
)
, [SubsLastYear] AS
(
	SELECT Region, COUNT([Organisation ID]) AS 'No of Subs LY' FROM LiveSubs
	WHERE [Start Date] &lt;= @MonthEndLastYear and [Expire Date] &gt; @MonthEndLastYear
	GROUP BY Region
)
, [SubsThisYear] AS
(
	SELECT Region, COUNT([Organisation ID]) AS 'No of Subs CY' FROM LiveSubs
	WHERE [Start Date] &lt;= @MonthEndThisYear and [Expire Date] &gt; @MonthEndThisYear
	GROUP BY Region
)
SELECT sly.Region, sly.[No of Subs LY], scy.[No of Subs CY]
FROM [SubsLastYear] sly
FULL OUTER JOIN [SubsThisYear] scy ON scy.Region = sly.Region</CommandText>
      </Query>
      <Fields>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="No_of_Subs_LY">
          <DataField>No of Subs LY</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="No_of_Subs_CY">
          <DataField>No of Subs CY</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
