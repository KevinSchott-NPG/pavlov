        <CommandText>--recursive CTE to generate a record for each available year by title
--parameterised by sales_rep/company
--future improvement - year_pub storage
--nature only

--OPRA-1337-Consortia data added
--OPRA-1041-Svenja issues

--test variables, comment out when not in use
/*
DECLARE @pt_access_type int;
SET @pt_access_type = 0;

DECLARE @pt_company int;
SET @pt_company = 1091;

DECLARE @pt_product varchar(100);
SET @pt_product = 'Nature Biotechnology';
*/

WITH cte_year ([Product Group Key], [Product Title], year_pub, archive)
AS
(
	SELECT pg.[Product Group Key],pg.[Product Title]	
		,1997 AS 'first_year'	--in lieu of table for year of first publication,  
		,9999 --code for non-archive
	FROM dim.ProductGroup pg		
	WHERE pg.[Product Group Type] = 'DW' 
		and pg.[Product Title] in ('Nature')		

	UNION ALL
	 
	SELECT [Product Group Key], [Product Title],  year_pub + 1,9999
	FROM cte_year
	WHERE year_pub &lt; year(getdate())
		
)

--	
SELECT bfg.year_pub
	, bfg.[Organisation ID] --OPRA-1401
	, bfg.[SID2 ID], bfg.[Full Name],bfg.Country
	, bfg.[Product Group Key]	
	, bfg.[Organisation Class1] 
		+ CASE WHEN bfg.[Organisation Class2] Is not null THEN ' - ' + bfg.[Organisation Class2] END 
		+ CASE WHEN bfg.[Organisation Class3] Is not null THEN ' - ' + bfg.[Organisation Class3] END	
		AS 'organisation_class'
	, bfg.consortia
	, sr.sales_rep	
	, 'Nature' as [Product Title]
	, 1869 as 'launch_date'
	, cs.max_start as 'current_sl_start'
	, cs.max_expire
	, esl.min_start as 'first_sl_start'
	, MAX(bfg.code) as 'max_code'
	, bfg.archive
	, CASE
		WHEN bfg.[Is Country] = 0 THEN bfg.State
		ELSE null
	END AS 'state'
	, CASE	
		WHEN ra.[Organisation ID] is null THEN 0
		ELSE 1
	END AS 'retained_access'	
	, CASE	
		WHEN csl.[Organisation ID] is null THEN 0
		ELSE 1
	END AS 'continuous_site_licence'	
	, CASE	
		WHEN caf.[Organisation ID] is null THEN 0
		ELSE 1
	END AS 'current_access_fee'	
	, CASE	
		WHEN cs.[Organisation ID] is null THEN 0
		ELSE 1
	END AS 'current_site_licence'	
	
	--returns one code from group, based on evaluation order below
	, CASE	
		--grandfather rights
		WHEN bfg.year_pub &gt;= 1997 and bfg.year_pub &lt; 2007 and ra.[Organisation ID] is not null and csl.[Organisation ID] is  not null THEN 'GF'
		--current site licence
		WHEN bfg.year_pub = year(getdate()) and MAX(bfg.code) = 2 and ((caf.[Organisation ID] is not null) or (cs.[Organisation ID] is not null)) THEN 'SL'
		--post cancellation rights
		WHEN bfg.year_pub &lt;&gt; year(getdate()) and MAX(bfg.code)= 2 and ((caf.[Organisation ID] is not null) or (cs.[Organisation ID] is not null)) THEN 'PC'
		--back file
		WHEN MAX(bfg.code)= 1 and ((caf.[Organisation ID] is not null) or (cs.[Organisation ID] is not null)) THEN 'BF'
		--inactive back file
		WHEN MAX(bfg.code)= 1 and ((caf.[Organisation ID] is  null) and  (cs.[Organisation ID] is  null)) THEN '(BF)'
		--inactive post cancellation rights
		WHEN MAX (bfg.code)= 2 and ((caf.[Organisation ID] is  null) and  (cs.[Organisation ID] is  null)) THEN '(PC)'
		--archive		
		WHEN MAX (bfg.code)= 3 and ((caf.[Organisation ID] is not null) or (cs.[Organisation ID] is not null)) THEN 'AR'
		--inactive archive
		WHEN MAX (bfg.code) = 3 and (bfg.archive &lt;&gt; 0) and  ((caf.[Organisation ID] is  null) and  (cs.[Organisation ID] is  null)) THEN '(AR)'		
		ELSE '(P)'
	END as 'code'	
FROM
	(
		SELECT cy.*, o.[Full Name]
			, o.[Organisation ID] --OPRA-1401
			, oc.Name
			, oc.[Organisation Class1]
			, oc.[Organisation Class2]
			, oc.[Organisation Class3]
			, o.[SID2 ID]
			, p.[Start Date], p.[Expire Date]
			, p.[Subscription Category]
			, l.Country		
			, l.State
			, l.[Is Country]	
			, c.consortia

			--code
			, CASE	
				--AR
				WHEN p.[Subscription Category] = 'Archive'
					and (p.[Start Date] = CAST('1 January' + CAST(cy.year_pub as varchar)as datetime)) 
					and (p.[Expire Date] = CAST('31 December' + CAST(cy.archive as varchar)as datetime)) 
				THEN 3 --'AR'
				--AR for 1997-2006
				WHEN p.[Subscription Category] = 'Archive'
					and (p.[Start Date] &lt;= CAST('31 December' + CAST(cy.year_pub as varchar)as datetime))
					and (p.[Expire Date] &gt;= CAST('1 January' + CAST(cy.year_pub as varchar)as datetime)) 
				THEN  3 --'AR'
				-- SL
				WHEN p.[Subscription Category] = 'Site Licence' 
					and (p.[Start Date] &lt;= CAST('31 December' + CAST(cy.year_pub as varchar)as datetime)) 
					and (p.[Expire Date] &gt;= CAST('1 January' + CAST(cy.year_pub as varchar)as datetime)) 
				THEN 2 --'SL' 
				-- backfile
				WHEN p.[Subscription Category] = 'Back File' 
					and (p.[Start Date] &lt;= CAST('31 December' + CAST(cy.year_pub as varchar)as datetime)) 
					and (p.[Expire Date] &gt;= CAST('1 January' + CAST(cy.year_pub as varchar)as datetime)) 
				THEN 1 --'BF'				
				ELSE 0 --' '		
			END as 'code'
			
		FROM --cte_year cy
			(
				SELECT *
				FROM cte_year
				UNION SELECT 224 as 'Product Group Key', 'Nature Archive 1869-1949' as 'Product Title', 1869 as 'year_pub',1949 as 'archive'
				UNION SELECT 225 as 'Product Group Key', 'Nature Archive 1950-1986' as 'Product Title', 1950 as 'year_pub',1986 as 'archive'
				UNION SELECT 226 as 'Product Group Key', 'Nature Archive 1987-1996' as 'Product Title', 1987 as 'year_pub',1996 as 'archive'
				UNION SELECT 227 as 'Product Group Key', 'Nature Archive 1997-2006' as 'Product Title', 1997 as 'year_pub',2006 as 'archive'
			) as cy
			cross join Nature_Warehouse.[dim].Organisation o
			inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]	
			inner join Nature_Warehouse.[dim].OrganisationClass oc on o.[Organisation Class Key] = oc.[Organisation Class Key]		
			
			--consortia acronyms
			left join
			(
				SELECT co.[From Organisation Key], Left( co.Acro,Len(co.Acro)-1) as 'consortia'
				FROM
				(
					SELECT DISTINCT co2.[From Organisation Key],
						(
							SELECT DISTINCT co1.Acronym + ','
							FROM 
							(
								SELECT a.[From Organisation Key], c.[Consortium ID], c.Acronym
								FROm fact.Affiliations a
									inner join dim.Consortium c on a.[To Organisation Key] = c.[Organisation Key]
								WHERE a.[Affiliation Type ID] = 1 --member of consortium
							)co1
							WHERE co1.[From Organisation Key] = co2.[From Organisation Key]
							FOR XML PATH ('') 
						) acro
					FROM 
					(
						SELECT a.[From Organisation Key], c.[Consortium ID], c.Acronym
						FROm fact.Affiliations a
							inner join dim.Consortium c on a.[To Organisation Key] = c.[Organisation Key]
						WHERE a.[Affiliation Type ID] = 1 --member of consortium
					)co2
				) co
			) c on o.[Organisation Key] = c.[From Organisation Key]
						
			left join 
			(
				SELECT o.[Organisation ID], pg.[Product Group Key],o.[SID2 ID],
					s.[Order Header ID], s.[Order Item Seq],s.[Start Date],s.[Expire Date],s.[Cancellation Date],
					p.[Subscription Category]
				FROM Nature_Warehouse.[dim].Organisation o
					inner join Nature_Warehouse.[dim].CustomerAddress ca on o.[Organisation ID]=ca.[Organisation ID]
					inner join Nature_Warehouse.[fact].[Sales] s on s.[Ship To Customer Key]=ca.[Customer Key]
					inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
					inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
					inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]											
				WHERE o.[Organisation ID] in (@pt_company)	
					and pg.[Product Title] in ('Nature','Nature Kanji','Nature Archive 1869-1949','Nature Archive 1950-1986','Nature Archive 1987-1996','Nature Archive 1997-2006')					
					and p.[Revenue Method] &lt;&gt; 'Print'	--non-print products
					and s.[Cancellation Date] is null	--excludes cancelled orders 
					and o.[is current] = 1 --current orgs only					
			) p on o.[Organisation ID] = p.[Organisation ID] 
				--and cy.[Product Group Key] = p.[Product Group Key] --excluded for Nature, so Kanji is included
				and 
				(
					(						
						(p.[Start Date] &lt;= CAST('31 December' + CAST(cy.year_pub as varchar)as datetime)) 
						and (p.[Expire Date] &gt;= CAST('1 January' + CAST(cy.year_pub as varchar)as datetime))
					)
					Or
					(						
						(p.[Start Date] = CAST('1 January' + CAST(cy.year_pub as varchar)as datetime)) 
						and (p.[Expire Date] = CAST('31 December' + CAST(cy.archive as varchar)as datetime)) 
					)
				) 			
			
		WHERE o.[Organisation ID] in (@pt_company)
			and o.[is current] = 1 --current orgs only	
			--and o.[Organisation Key] in (@pt_company)	--01/08/2013	
			
	) bfg
	
	--sales rep
	left join 
	(
		SELECT osr.[Organisation ID], sr.Name as 'sales_rep'
		FROM fact.OrganisationSalesRep osr 
			inner join dim.SalesRep sr on osr.[Sales Rep Key] = sr.[Sales Rep Key]
		WHERE osr.[Is Current] = 1 -- current rep
			and osr.[Business Unit ID]= 1 -- NPG
	) sr on bfg.[Organisation ID] = sr.[Organisation ID]
	
	--retained access
	left join
	(
		SELECT distinct o.[Organisation ID]
		FROM Nature_Warehouse.[dim].Organisation o
			inner join Nature_Warehouse.[dim].CustomerAddress ca on o.[Organisation ID]=ca.[Organisation ID]
			inner join Nature_Warehouse.[fact].[Sales] s on s.[Ship To Customer Key]=ca.[Customer Key]
			inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
			inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
			inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]	
		WHERE s.[Cancellation Date] is null
			and s.[Start Date] &gt;= CAST('1 January 2006' as datetime)
			and s.[Expire Date] &lt; CAST('1 March 2007' as datetime)
			and p.[Subscription Category] in ('Site Licence')
			and o.[Organisation ID] in (@pt_company)
			and  pg.[Product Title] in ('Nature')	
			and o.[is current] = 1 --current orgs only	
	) ra on bfg.[Organisation ID] = ra.[Organisation ID]
	
	--continuous site licence
	left join 
	(
		SELECT o.[Organisation ID]
		FROM Nature_Warehouse.[dim].Organisation o
			inner join Nature_Warehouse.[dim].CustomerAddress ca on o.[Organisation ID]=ca.[Organisation ID]
			inner join Nature_Warehouse.[fact].[Sales] s on s.[Ship To Customer Key]=ca.[Customer Key]
			inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
			inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
			inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]	
		WHERE s.[Cancellation Date] is null
			and s.[Start Date] &gt;= CAST('1 January 2007' as datetime)
			--and s.[Expire Date] &gt; CAST('1 January 2007' as datetime)
			and p.[Subscription Category] in ('Site Licence')			
			and o.[Organisation ID] in (@pt_company)
			and  pg.[Product Title] in ('Nature')	
			and o.[is current] = 1 --current orgs only					
		GROUP BY o.[Organisation ID]
		HAVING COUNT(s.[Order Header ID]) &gt;= (YEAR(getdate()) - 2006)
	) csl on bfg.[Organisation ID] = csl.[Organisation ID]
	
	--current access fee	
	left join
	(		
		SELECT o.[Organisation ID]
		FROM Nature_Warehouse.[dim].Organisation o
			inner join Nature_Warehouse.[dim].CustomerAddress ca on o.[Organisation ID]=ca.[Organisation ID]
			inner join Nature_Warehouse.[fact].[Sales] s on s.[Ship To Customer Key]=ca.[Customer Key]
			inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
			inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
			inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]	
		WHERE s.[Cancellation Date] is null
			and s.[Start Date] &lt; GETDATE()
			and s.[Expire Date] &gt; GETDATE()
			and p.[Subscription Category] in ('Access Fee Fixed','Access Fee Custom Archve','Access Fee Custom PCA')		
			and o.[Organisation ID] in (@pt_company)
			and  pg.[Product Title] in ('Nature')	
			and o.[is current] = 1 --current orgs only			
		GROUP BY o.[Organisation ID],pg.[Product Group Key]
	) caf on bfg.[Organisation ID] = caf.[Organisation ID] 
	
	--current site licence
	left join
	(		
		SELECT o.[Organisation ID], max(s.[Start Date]) as 'max_start', max(s.[Expire Date]) as 'max_expire'
		FROM Nature_Warehouse.[dim].Organisation o
			inner join Nature_Warehouse.[dim].CustomerAddress ca on o.[Organisation ID]=ca.[Organisation ID]
			inner join Nature_Warehouse.[fact].[Sales] s on s.[Ship To Customer Key]=ca.[Customer Key]
			inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
			inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
			inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]	
		WHERE s.[Cancellation Date] is null
			and s.[Start Date] &lt; GETDATE()
			and s.[Expire Date] &gt; GETDATE()
			and p.[Subscription Category] in ('Site Licence')			
			and o.[Organisation ID] in (@pt_company)
			and  pg.[Product Title] in ('Nature')	
			and o.[is current] = 1 --current orgs only				
		GROUP BY o.[Organisation ID],pg.[Product Group Key]
	) cs on bfg.[Organisation ID] = cs.[Organisation ID] 		 	
	
	----earliest site licence
	left join
	(
		SELECT o.[Organisation ID], min(s.[Start Date]) as 'min_start'
		FROM Nature_Warehouse.[dim].Organisation o
			inner join Nature_Warehouse.[dim].CustomerAddress ca on o.[Organisation ID]=ca.[Organisation ID]
			inner join Nature_Warehouse.[fact].[Sales] s on s.[Ship To Customer Key]=ca.[Customer Key]
			inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
			inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
			inner join Nature_Warehouse.[dim].location l on o.[Location Key] = l.[Location Key]	
		WHERE s.[Cancellation Date] is null			
			and p.[Subscription Category] in ('Site Licence')		
			and o.[Organisation ID] in (@pt_company)
			and  pg.[Product Title] in ('Nature')	
			and o.[is current] = 1 --current orgs only				
		GROUP BY o.[Organisation ID]
	) esl on bfg.[Organisation ID] = esl.[Organisation ID] 
			
WHERE
	(
		@pt_access_type = 0
		Or @pt_access_type = 1 and ((cs.[Organisation ID] is not null) and (caf.[Organisation ID] is  null ))
		Or @pt_access_type = 2 and ((cs.[Organisation ID] is  null) and( caf.[Organisation ID] is not  null ))
		Or @pt_access_type = 3 and ((cs.[Organisation ID] is  null )and (caf.[Organisation ID] is  null ))
	)	
	 --and sr.sales_rep in (@pt_sales_rep) --01/08/2013	
	--and bfg.[SID2 ID] = 3569
	
GROUP BY bfg.year_pub
	, bfg.[Organisation ID] --OPRA-1401
	, bfg.[Full Name],bfg.Country, bfg.[SID2 ID]
	, bfg.[Product Group Key], bfg.[Product Title]
	, ra.[Organisation ID] --retained access
	, csl.[Organisation ID] --continuous site licence
	, caf.[Organisation ID] -- current access fee
	, cs.[Organisation ID] --current site licence
	, bfg.archive
	, bfg.Name
	, bfg.[Organisation Class1]
	, bfg.[Organisation Class2]
	, bfg.[Organisation Class3]
	, sr.sales_rep
	, cs.max_start
	, cs.max_expire
	, bfg.State
	, bfg.[Is Country]
	, esl.min_start
	, bfg.consortia
ORDER BY bfg.[Organisation ID],bfg.archive, bfg.year_pub</CommandText>
      </Query>
