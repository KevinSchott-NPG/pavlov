  <DataSets>
    <DataSet Name="dt_content_holdings">
      <Query>
        <DataSourceName>dt_dpt_warehouse_live</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pt_company">
            <Value>=Parameters!pt_company.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pt_product">
            <Value>=Parameters!pt_product.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT o.[SID2 ID],
	o.[Organisation ID] AS Id,
	o.[Full Name],
	pg.[Product Title],
	COUNT(pg.[Product Title ID]) AS Journals,
	sum(s.[SID Net Base Value]) AS Total,
	sum(s.[Order Item Net Local Value]) AS TotalLocal,
	max(c.[Currency Code]) AS LocalCurrency,
	CASE WHEN (s.[Start Date] &lt; getdate() AND s.[Expire Date] &gt; getdate()) THEN 1 ELSE 0 END AS IsCurrent,
	CASE WHEN (s.[Start Date] &lt; DATEADD(yy, -1, getdate()) AND s.[Expire Date] &gt; DATEADD(yy, -1, getdate())) THEN 1 ELSE 0 END AS IsLY,
	CASE WHEN s.[Start Date] &gt; getdate() THEN 1 ELSE 0 END AS IsFuture,
	s.[Start Date],
	s.[Expire Date],
	p.[Subscription Category],
	s.[Order Header ID],
	p.[Revenue Method]
FROM Nature_Warehouse.[fact].[Sales] s
	inner join Nature_Warehouse.[dim].CustomerAddress ca on s.[Ship To Customer Key]=ca.[Customer Key]
	inner join Nature_Warehouse.[dim].Organisation o on o.[Organisation Key]=ca.[Organisation Key]
	inner join Nature_Warehouse.[dim].OrganisationClass oc on o.[Organisation Class Key] = oc.[Organisation Class Key]
	inner join Nature_Warehouse.[dim].Product p on s.[Unpackaged Product Key]=p.[Product Key]
	inner join Nature_Warehouse.[dim].medium m on p.[Medium Key]=m.[Medium Key] 
	inner join Nature_Warehouse.[dim].ProductGroup pg on p.[DW Product Group Key]=pg.[Product Group Key]
	inner join Nature_Warehouse.[dim].Currency c on c.[Currency Key] = s.[Currency Key]
WHERE o.[SID2 ID] = @pt_company
	and 
	(
		(@pt_product = 'Nature' and pg.[Product Title]in ('Nature','Nature Kanji','Nature Archive 1869-1949','Nature Archive 1950-1986','Nature Archive 1987-1996','Nature Archive 1997-2006'))	
		--Or (@pt_product = 'Sciam' and pg.[Product Title]
		Or (pg.[Product Title] = @pt_product)
	)	
	
	--AND
	--(
	--	p.[Subscription Category]  in ('Site Licence', 'Archive','Back File')
	--	Or 
	--	(
	--		p.[Subscription Category]  in ('Access Fee Custom Archve','Access Fee Fixed')
	--		AND s.[Start Date] &lt; getdate() AND s.[Expire Date] &gt; getdate()	
	--	)
	--)     
GROUP BY o.[Organisation ID],
	o.[Full Name],
    pg.[Product Title],
    s.[Start Date],
    s.[Expire Date],
