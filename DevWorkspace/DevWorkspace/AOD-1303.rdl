        <CommandText>
-- Main Statement
select 
ppvo.ppv_order_id
,pog.created_on
,sa.site_id
,ca.company as Eclipse_Cust_Name
,oi.customer_id as Eclipse_Cust_ID
--,op.ord_net_base_total as Price
--,op.ord_currency as Order_Currency
,ppvo.units_total as Units_Purchased
,oi.order_date as Date_Of_Purchase
,case 
	when oc.description not like '%mediated%' 
	then 0
	else 1
end as 'Mediated'
,ppvo.units_used as units_used
,p.product_desc


from
rptsql1.Site_Licenses2_reporting.tango_client.site_accounts sa
inner join rptsql1.Site_Licenses2_reporting.tango_client.licenses l 
	on sa.site_id= l.site_id 
inner join rptsql1.Site_Licenses2_reporting.dbo.ppv_orders ppvo 
	on ppvo.license_id = l.id
inner join rptsql1.Site_Licenses2.dbo.ppv_order_grant pog 
	on pog.ppv_order_id = ppvo.ppv_order_id
inner join rptsql1.reg_data.tango_client.products p
	on p.product_code = pog.journal_code
inner join order_item oi
	on oi.order_item_seq = l.eclipse_line_number 
	and oi.orderhdr_id = l.eclipse_order_number
inner join customer_address ca
	on oi.customer_id = ca.customer_id 
--inner join view_mcmn_order_payment op
--	on op.orderhdr_id = oi.orderhdr_id
inner join oc
	on oc.oc_id = oi.oc_id

where 
not ca.company is null
and oi.customer_id 
	not in (3938689, 3823245, 3824551, 3935787, 3038092, 3821145, 4003802)
and pog.created_on &lt;= @enddate 
and pog.created_on &gt;= @startdate
and sa.site_id = 4690

group by
ca.company
,sa.site_id
,oi.customer_id
--,op.ord_net_base_total
--,op.ord_currency
,ppvo.units_total
,oi.order_date
,oc.description
,ppvo.units_used
,ppvo.ppv_order_id
,p.product_desc
,pog.created_on

order by
sa.site_id
,pog.created_on asc
--ca.company
--,oi.order_date asc</CommandText>
