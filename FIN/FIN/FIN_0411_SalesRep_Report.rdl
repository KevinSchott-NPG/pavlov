    <DataSet Name="Hyperion_salesrep">
      <Query>
        <DataSourceName>ds_dpt_warehouse_live</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SR_Status">
            <Value>=Parameters!SR_Status.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
----------------------------------------------
;WITH previous_week_assignment AS (
		SELECT
			 ExtractedOn AS Prev_week
			,OrganisationId AS Prev_OrgID
			,SalesRepId AS Prev_SalesRepId
			,current_week_assignment.SID2SalesRepId as Prev_SID2SalesRepID
			,SR.SIDLocation AS Prev_Rep_location
		FROM [Nature_Master].[md].[SalesRepAssignmentHistory] AS current_week_assignment
		LEFT OUTER JOIN
             [Nature_Master].[md].SalesRep AS SR ON SR.SID2SalesRepId = current_week_assignment.SID2SalesRepId
		WHERE CAST(CONVERT(VARCHAR, ExtractedOn, 102) AS DATETIME) = DATEADD(wk, DATEDIFF(wk,0,@StartDate), 0)
	)
	,current_week_assignment AS (
		SELECT
			 ExtractedOn AS Current_week
			,OrganisationId AS Current_OrgID
			,SalesRepId AS Current_SalesRepId
			,current_week_assignment.SID2SalesRepId as Current_SID2SalesRepID
			,SR.SIDLocation AS Current_Rep_location
		FROM [Nature_Master].[md].[SalesRepAssignmentHistory] AS current_week_assignment
		LEFT OUTER JOIN
             [Nature_Master].[md].SalesRep AS SR ON SR.SID2SalesRepId = current_week_assignment.SID2SalesRepId
		WHERE CAST(CONVERT(VARCHAR, ExtractedOn, 102) AS DATETIME) = DATEADD(wk, DATEDIFF(wk,0,@EndDate), 0)
	)
SELECT DISTINCT
	 Prev_week as PreviousDate
	--,Prev_OrgID
	--,prevOrg.SID2_ID as Prev_SID2ID
	--,prevOrg.OriginalName AS PreviousOrganisation
	,Current_week as CurrentDate
	,Current_OrgID
	,currOrg.SID2_ID as Curr_SID2ID
	,currOrg.OriginalName as CurrentOrganisation
	,Prev_SID2SalesRepID
	,prevSR.FirstName+' '+prevSR.LastName as PreviousSalesRep
	,Prev_Rep_location
	,Current_SID2SalesRepID
	,currSR.FirstName+' '+currSR.LastName as CurrentSalesRep
	,Current_Rep_location
	,CASE
		WHEN (Prev_SalesRepId is NULL and Current_SalesRepId is not NULL) THEN 'Newly Assigned'
		WHEN (Prev_SalesRepId is not NULL and (Current_SalesRepId &lt;&gt; Prev_SalesRepId)) THEN 'Assignments changed'
		WHEN (Prev_SalesRepId is not NULL and (Current_SalesRepId = Prev_SalesRepId)) THEN 'No Change'
		WHEN Current_SalesRepId is NULL THEN 'Not Assigned'
		ELSE 'Status Unknown'
	 END AS Status
FROM current_week_assignment as a
FULL JOIN previous_week_assignment as b ON
	Prev_OrgId = Current_OrgId
left join Nature_Master.md.Organisation as prevOrg on
b.Prev_OrgID = prevOrg.Id
left join Nature_Master.md.Organisation as currOrg on
a.Current_OrgID = currOrg.id
left join Nature_Master.md.SalesRep as prevSR on
b.Prev_SalesRepId = prevSR.id
left join Nature_Master.md.SalesRep as currSR on
a.Current_SalesRepId = currSR.Id
WHERE currOrg.OriginalName is not NULL
and CASE
		WHEN (Prev_SalesRepId is NULL and Current_SalesRepId is not NULL) THEN 'Newly Assigned'
		WHEN (Prev_SalesRepId is not NULL and (Current_SalesRepId &lt;&gt; Prev_SalesRepId)) THEN 'Changed'
		WHEN (Prev_SalesRepId is not NULL and (Current_SalesRepId = Prev_SalesRepId)) THEN 'No Change'
		WHEN Current_SalesRepId is NULL THEN 'Not Assigned'
		ELSE 'Status Unknown'
	 END 
in (@SR_Status)
ORDER BY 8,3

</CommandText>
