  <DataSets>
    <DataSet Name="dt_campaign_totals">
      <Query>
        <DataSourceName>ds_eclipse_reporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@source_code">
            <Value>=Parameters!source_code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT (CAST(month(order_date) as varchar) + '-' + CAST(year(order_date) as varchar)) as 'order_date_month', oc, count(distinct orderhdr_id) as 'count_orders'
FROM order_item oi
	inner join oc on oi.oc_id = oc.oc_id
	inner join source_code sco on oi.source_code_id = sco.source_code_id
WHERE sco.source_code in (@source_code)
	and order_item_type &lt;&gt; 4
GROUP BY (CAST(month(order_date) as varchar) + '-' + CAST(year(order_date) as varchar)), oc</CommandText>
      </Query>
      <Fields>
        <Field Name="order_date_month">
          <DataField>order_date_month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="oc">
          <DataField>oc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="count_orders">
          <DataField>count_orders</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dt_campaign_source_codes">
      <Query>
        <DataSourceName>ds_eclipse_reporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@source_code">
            <Value>=Parameters!source_code.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT Datename(month,order_date) + ' ' + CAST(year(order_date) as varchar) as 'Order Date', oc as 'Journal', source_code as 'Source Code', orderhdr_id as 'Count of Orders'
FROM order_item oi
	inner join oc on oi.oc_id = oc.oc_id
	inner join source_code sco on oi.source_code_id = sco.source_code_id
WHERE sco.source_code in (@source_code)
	and order_item_type &lt;&gt; 4</CommandText>
      </Query>
      <Fields>
        <Field Name="Order_Date">
          <DataField>Order Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Journal">
          <DataField>Journal</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Source_Code">
          <DataField>Source Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Count_of_Orders">
          <DataField>Count of Orders</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
