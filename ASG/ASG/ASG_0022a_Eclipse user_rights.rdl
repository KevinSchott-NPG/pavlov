  <DataSets>
    <DataSet Name="user_group">
      <Query>
        <DataSourceName>ds_eclipse_live</DataSourceName>
        <CommandText>--@user_group
SELECT 
	distinct ug.user_group
FROM user_group ug
	inner join user_group_rights ugr on ug.user_group = ugr.user_group
	inner join right_desc rd on ugr.right_desc_id = rd.right_desc_id
	left join oc on ugr.oc_id = oc.oc_id
	left join report_category rc on ugr.report_category_id = rc.report_category_id
	INNER JOIN user_code user_code ON ug.user_group=user_code.user_group</CommandText>
      </Query>
      <Fields>
        <Field Name="user_group">
          <DataField>user_group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="user_group_description">
      <Query>
        <DataSourceName>ds_eclipse_live</DataSourceName>
        <CommandText>--@user_group_description
SELECT 
	distinct ug.description
FROM user_group ug
	inner join user_group_rights ugr on ug.user_group = ugr.user_group
	inner join right_desc rd on ugr.right_desc_id = rd.right_desc_id
	left join oc on ugr.oc_id = oc.oc_id
	left join report_category rc on ugr.report_category_id = rc.report_category_id
	INNER JOIN user_code user_code ON ug.user_group=user_code.user_group</CommandText>
      </Query>
      <Fields>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Eclipse_user_rights">
      <Query>
        <DataSourceName>ds_eclipse_live</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@user_group">
            <Value>=Parameters!user_group.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@user_group_description">
            <Value>=Parameters!user_group_description.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@usercode">
            <Value>=Parameters!usercode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@user_name">
            <Value>=Parameters!user_name.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> ---------------------------
 --Eclipse User Group and Rights

---------------------------
--Access Rights by User
---------------------------
SELECT 
	ug.user_group
	, ug.description as user_group_description
	, user_code.user_code as 'user_code'
	, user_code.name as 'user_name'
	, oc.oc_id
	, oc.oc
	, CASE WHEN SUBSTRING(rd.description, 1, CHARINDEX(',', rd.description)) = 'Applications,' THEN 1 ELSE 0 END AS [Applications]
	, CASE WHEN SUBSTRING(rd.description, 1, CHARINDEX(',', rd.description)) = 'Batch,' THEN 1 ELSE 0 END AS [Batch]
	, CASE WHEN SUBSTRING(rd.description, 1, CHARINDEX(',', rd.description)) = 'Processing,' THEN 1 ELSE 0 END AS [Processing]
	, CASE WHEN SUBSTRING(rd.description, 1, CHARINDEX(',', rd.description)) = 'Report Mgr.,' THEN 1 ELSE 0 END AS [Report Mgr]
	, CASE WHEN SUBSTRING(rd.description, 1, CHARINDEX(',', rd.description)) = 'Reports,' THEN 1 ELSE 0 END AS [Reports]
	, CASE WHEN SUBSTRING(rd.description, 1, CHARINDEX(',', rd.description)) = 'Transactions,' THEN 1 ELSE 0 END AS [Transactions]
	, rd.description as detailed_rights_description
	, rc.description as report_manager_group


FROM user_group ug
	inner join user_group_rights ugr on ug.user_group = ugr.user_group
	inner join right_desc rd on ugr.right_desc_id = rd.right_desc_id
	left join oc on ugr.oc_id = oc.oc_id
	left join report_category rc on ugr.report_category_id = rc.report_category_id
	INNER JOIN user_code user_code ON ug.user_group=user_code.user_group

WHERE 
	(user_code.locked_out&lt;&gt;1 OR user_code.disabled&lt;&gt;1)
	and ug.user_group  in(@user_group)
	and ug.description  in(@user_group_description)
	and (LOWER (user_code.user_code) like '%' + @usercode + '%'
		OR (user_code.user_code) like '%' + @usercode + '%')
	and (LOWER (user_code.name) like '%' + @user_name + '%'
		OR (user_code.name) like '%' + @user_name + '%')

Order by 
	user_code.name
	, rd.description</CommandText>
      </Query>
