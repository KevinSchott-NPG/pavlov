      <Query>
        <DataSourceName>Campaign</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
      [offer_id]	as [Offer ID]  
      ,[state]     as [Status]
      ,[start_date] as [Start Date]
      ,[end_date] as  [End Date]
      ,[priority_code] as [Campaign Code]
      ,[product_code] as [Product Code]
      ,[offer_description] as [Offer Description]
      ,[regular_type_code] as [Regular Type Code]
      ,[sub_type] as[Subscription Type]
      ,[region] as [Region]
      ,[sub_region] as [SubRegion]
      ,[currency] as  [Currency]
      ,[offer_price] as [Offer Price]
      ,cast ([original_price]as int) as [Original Price]
      ,[discount] as [Discount Price]
      ,[created_by] as [Created by]
      ,[updated_by] as [Updated by]
  FROM [Campaign].[dbo].[offer]
  where LOWER ([priority_code]) like '%' + @PrioCode + '%'
		OR ([priority_code]) like '%' + @PrioCode + '%'
  Order by [start_date] Desc, [end_date] Desc, [priority_code], [offer_id]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@PrioCode">
            <Value>=Parameters!PrioCode.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
