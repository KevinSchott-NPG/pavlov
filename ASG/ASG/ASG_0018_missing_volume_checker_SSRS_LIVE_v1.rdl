  <Description>Report returns all OCs that have issues in the current year, but not in the year supplied as a parameter</Description>
  <Author>OPRA</Author>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="ds_eclipse_live">
      <DataSourceReference>ds_eclipse_live</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>803b4a6b-a98f-4567-b47d-ca4f0615adc7</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dt_missing_volume_checker">
      <Query>
        <DataSourceName>ds_eclipse_live</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        oc.oc, oc.description, oc.oc_id, oc.profit_center, oc_p.oc AS 'parent_oc', oc_gp.oc AS 'grand_parent_oc', oc_ggp.oc AS 'great_grand_parent_oc', 
                         @year AS 'volume_year'
FROM            oc INNER JOIN
                         oc AS oc_p ON oc.parent_oc_id = oc_p.oc_id INNER JOIN
                         oc AS oc_gp ON oc_p.parent_oc_id = oc_gp.oc_id INNER JOIN
                         oc AS oc_ggp ON oc_gp.parent_oc_id = oc_ggp.oc_id
WHERE        (oc.oc_id NOT IN
                             (SELECT        i.oc_id
                               FROM            issue AS i LEFT OUTER JOIN
                                                         volume_group AS vg ON i.volume_group_id = vg.volume_group_id
                               WHERE        (vg.volume = @year))) AND (oc.oc_id IN
                             (SELECT        i.oc_id
                               FROM            issue AS i LEFT OUTER JOIN
                                                         volume_group AS vg ON i.volume_group_id = vg.volume_group_id
                               WHERE        (vg.volume = YEAR(GETDATE())))) AND (oc.oc_type = 1) AND (NOT (oc_p.oc LIKE '%x%OLD%')) AND (NOT (oc_gp.oc LIKE '%x%OLD%')) AND 
                         (NOT (oc_ggp.oc LIKE '%x%OLD%'))
ORDER BY oc.oc</CommandText>
      </Query>
      <Fields>
        <Field Name="oc">
          <DataField>oc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="description">
          <DataField>description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="oc_id">
          <DataField>oc_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="profit_center">
          <DataField>profit_center</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="parent_oc">
          <DataField>parent_oc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="grand_parent_oc">
          <DataField>grand_parent_oc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="great_grand_parent_oc">
          <DataField>great_grand_parent_oc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="volume_year">
          <DataField>volume_year</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
